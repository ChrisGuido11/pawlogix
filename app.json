{
  "expo": {
    "name": "PawLogix",
    "slug": "pawlogix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "pawlogix",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0D7377"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pawlogix.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0D7377"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.pawlogix.app",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-dev-client",
      "expo-router",
      "expo-secure-store",
      [
        "expo-camera",
        {
          "cameraPermission": "PawLogix needs camera access to scan your pet's vet records."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "PawLogix needs photo access to upload your pet's vet records."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#0D7377"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "004aa69e-2168-4c1a-9a58-7cf0aa23651b"
      }
    },
    "owner": "crisguido",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/004aa69e-2168-4c1a-9a58-7cf0aa23651b"
    }
  }
}
